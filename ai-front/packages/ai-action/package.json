{
  "name": "@maxpro/ai-action",
  "version": "1.0.0",
  "description": "A flexible Vue 3 component for AI assistant integration with hover interaction and draggable modal",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "development": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "ai",
    "assistant",
    "hover",
    "action",
    "draggable",
    "modal",
    "iframe",
    "typescript"
  ],
  "author": {
    "name": "MaxPro Team",
    "email": "support@ai1center.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-action"
  },
  "bugs": {
    "url": "https://github.com/your-org/ai-action/issues"
  },
  "homepage": "https://github.com/your-org/ai-action#readme",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.11",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/ai-action.es.js",
        "require": "./dist/ai-action.umd.js"
      },
      "./dist/ai-action.css": "./dist/ai-action.css",
      "./dist/style.css": "./dist/ai-action.css"
    }
  }
}

